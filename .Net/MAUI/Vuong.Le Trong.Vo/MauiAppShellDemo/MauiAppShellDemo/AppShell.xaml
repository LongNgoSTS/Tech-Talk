<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MAUIAppDemo.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MAUIAppDemo"
    xmlns:page="clr-namespace:MauiAppShellDemo.Pages"
    >

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
    <ShellContent
        Title="Datetime"
        ContentTemplate="{DataTemplate page:DateTimePage}"
        Route="Datetime" />
    <ShellContent
        Title="DogPage"
        ContentTemplate="{DataTemplate page:DogBreedsPage}"
        Route="DogPage" />
    <ShellContent
        Title="Chat"
        ContentTemplate="{DataTemplate page:Chat}"
        Route="Chat" />
</Shell>
